{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["DOM", "ES2018"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["obsidian", "node"]
  },
  "include": ["src/**/*"]
}
